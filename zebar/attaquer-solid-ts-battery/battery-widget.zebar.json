{
  "$schema": "https://github.com/glzr-io/zebar/raw/v2.1.0/resources/widget-schema.json",
  "htmlPath": "./dist/index.html",
  "zOrder": "bottom_most",
  "shownInTaskbar": false,
  "focused": false,
  "resizable": false,
  "transparent": true,
  "presets": [
    {
      "name": "default",
      "anchor": "top_left",
      "offsetX": "50px",
      "offsetY": "600px",
      "width": "200px",
      "height": "170px",
      "monitorSelection": {
        "type": "all"
      }
    }
  ]
}
