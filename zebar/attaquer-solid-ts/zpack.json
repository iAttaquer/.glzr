{
  "$schema": "https://github.com/glzr-io/zebar/raw/v3.1.0/resources/zpack-schema.json",
  "name": "attaquer-solid-ts",
  "version": "0.0.0",
  "description": "",
  "tags": [],
  "previewImages": [],
  "repositoryUrl": "",
  "widgets": [
    {
      "name": "top-bar",
      "htmlPath": "./dist/index.html",
      "zOrder": "normal",
      "shownInTaskbar": false,
      "focused": false,
      "resizable": false,
      "transparent": true,
      "includeFiles": [
        "**/**"
      ],
      "caching": {
        "defaultDuration": 604800,
        "rules": []
      },
      "privileges": {
        "shellCommands": [
          {
            "program": "glazewm",
            "argsRegex": "shell-exec | %programfiles%/SystemInformer/SystemInformer/.exe"
          },
          {
            "program": "C:\\Program Files\\SystemInformer\\SystemInformer.exe",
            "argsRegex": ""
          },
          {
            "program": "explorer.exe",
            "argsRegex": "ms-actioncenter://"
          },
          {
            "program": "taskmgr",
            "argsRegex": ""
          },
          {
            "program": "C:\\Program Files\\Mem Reduct\\memreduct.exe",
            "argsRegex": ""
          }
        ]
      },
      "presets": [
        {
          "name": "default",
          "anchor": "top_left",
          "offsetX": "0px",
          "offsetY": "0px",
          "width": "100%",
          "height": "26px",
          "monitorSelection": {
            "type": "all"
          },
          "dockToEdge": {
            "enabled": false,
            "edge": null,
            "windowMargin": "0px"
          }
        }
      ]
    }
  ]
}
