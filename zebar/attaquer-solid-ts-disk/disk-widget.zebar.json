{
  "$schema": "https://github.com/glzr-io/zebar/raw/v2.1.0/resources/widget-schema.json",
  "htmlPath": "./dist/index.html",
  "zOrder": "bottom_most",
  "shownInTaskbar": false,
  "focused": false,
  "resizable": true,
  "transparent": true,
  "presets": [
    {
      "name": "default",
      "anchor": "top_left",
      "offsetX": "1300px",
      "offsetY": "500px",
      "width": "200px",
      "height": "270px",
      "monitorSelection": {
        "type": "all"
      },
      "dockToEdge": {
        "enabled": false,
        "edge": null,
        "windowMargin": "0px"
      }
    }
  ]
}
